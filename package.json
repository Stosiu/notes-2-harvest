{
  "name": "notes-2-harvest",
  "version": "0.0.1",
  "description": "Make uploading lots of harvest entries easy.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts ./__tests__/files/example-note.txt",
    "format:check": "prettier --check './src/**/*.{js,ts}' --check './__tests__/**/*.{js,ts}'",
    "format:write": "prettier --write './src/**/*.{js,ts}' --write './__tests__/**/*.{js,ts}'",
    "eslint": "eslint ./src/ --ext js,jsx,ts,tsx",
    "eslint:fix": "eslint ./src/ --ext js,jsx,ts,tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "ci:test": "jest --ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "__tests__/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "harvest",
    "notes",
    "automation"
  ],
  "author": {
    "name": "Aleksander St√≥s",
    "email": "aleksander.stos@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/configstore": "^4.0.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "configstore": "^5.0.1",
    "dayjs": "^1.8.33",
    "harvest": "^2.2.5"
  }
}
