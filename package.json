{
  "name": "notes2harvest",
  "version": "1.0.0",
  "description": "Make uploading lots of Harvest entries easy.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Stosiu/notes-2-harvest.git"
  },
  "engines": {
    "node": ">= 12.18.3"
  },
  "type": "commonjs",
  "files": [
    "lib/**/*",
    "typings/**/*.d.ts"
  ],
  "types": "./typings/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./lib/": "./lib/",
    "./": "./lib/"
  },
  "bin": {
    "notes2harvest": "./lib/index.js"
  },
  "scripts": {
    "build": "rm -rf lib && rm -rf typings && tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon -r tsconfig-paths/register --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "format:check": "prettier --check './src/**/*.{js,ts}' --check './__tests__/**/*.{js,ts}'",
    "format:write": "prettier --write './src/**/*.{js,ts}' --write './__tests__/**/*.{js,ts}'",
    "eslint": "eslint ./src/ --ext js,jsx,ts,tsx",
    "eslint:fix": "eslint ./src/ --ext js,jsx,ts,tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "ci:test": "jest --ci",
    "lint": "npm run eslint && npm run format:check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "__tests__/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "harvest",
    "notes",
    "automation"
  ],
  "author": {
    "name": "Aleksander St√≥s",
    "email": "aleksander.stos@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/cli-progress": "^3.8.0",
    "@types/configstore": "^4.0.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "commander": "^6.0.0",
    "configstore": "^5.0.1",
    "dayjs": "^1.8.33",
    "harvest": "^2.2.5"
  }
}
